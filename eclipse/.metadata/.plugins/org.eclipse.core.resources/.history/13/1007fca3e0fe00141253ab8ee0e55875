package de.tisan.forge.firstmod.proxies;

public class FirstModServerProxy extends FirstModProxy {

	@Override
	public void preInit() {

	}

	@Override
	public void init() {

	}

	@Override
	public void postInit() {

	}

	@Override
	public EntityPlayer getClientPlayer() {
		return null;
	}

	@Override
	public void registerRenderers() {

	}

	@Override
	public EntityPlayer getPacketPlayer(final INetHandler inh) {
		if (inh instanceof NetHandlerPlayServer) {
			return ((NetHandlerPlayServer) inh).playerEntity;
		}
		return null;
	}
}
