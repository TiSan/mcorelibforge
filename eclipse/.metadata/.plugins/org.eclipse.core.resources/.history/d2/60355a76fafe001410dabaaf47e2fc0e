package de.tisan.forge.firstmod.crafting;

import net.minecraft.item.ItemStack;
import cpw.mods.fml.common.registry.GameRegistry;
import de.tisan.forge.firstmod.blocks.FirstModBlocks;
import de.tisan.forge.firstmod.items.FirstModItems;

public class CraftingManager {
	
	
	public static void load() {
		GameRegistry.addShapedRecipe(new ItemStack(FirstModBlocks.get().getBlockTiBlock(), 1), " e ", "h h", "   ", Character.valueOf('e'));
	}
}
